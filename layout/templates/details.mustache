<div class="container flexContainer">
	{{{sideBar}}}
	<div class="content flex5">
		<a href="index.html">
			<button class="return">Return to Summary</button>
		</a>
		<h2 class="projectName">
		  <a  class="projectURL" href="{{project_URL}}" target="_blank">
			{{project_name}}
		  </a>
		</h2>
		<p class="projectDescription">{{project_description}}</p>
		<div id="toggleButtons">
  		  <button id="yearButton" class="active">Year</button>
  			<button id="monthButton">Month</button>
  			<button id="weekButton">Week</button>
		</div>
		<section id="lineAssessments" class="full">
			<div class="header">
				<h3>Project Timeline Summaries</h3>
				<div class="information">
					{{{informationImg}}}
					<div class="informationContent">
						{{{info_session_time}}}
					</div>
				</div>
			</div>
			<div class="chartArea">
			  <span class="year figure">
			    <h3>Year</h3>
				  {{{year_line_sessions}}}
				</span>
				<span class="month figure">
			    <h3>Month</h3>
				  {{{month_line_sessions}}}
				</span>
				<span class="week figure">
			    <h3>Week</h3>
				  {{{week_line_sessions}}}
				</span>
			</div>
		</section>
		<section class="half flexContainer">
			<div class="flex1 flexBorder">
							<div class="header">
					<h3>Geographic Locations</h3>
					<div class="information">
					  {{{informationImg}}}
					  <div class="informationContent">
						{{{info_geo}}}
					  </div>
					</div>
				</div>
				<div class="chartArea">
					{{{viz_geo_apps_year}}}
				</div>
			</div>
			<div class="flex1">
			  <div class="header">
					<h3>Traffic Sources</h3>
					<div class="information">
						{{{informationImg}}}
						<div class="informationContent">
						  {{{info_source}}}
						</div>
					</div>
				</div>
				<div class="chartArea">
					{{{viz_source_year}}}
				</div>
			</div>
		</section>
		<section class="half flexContainer">
			<div class="flex1 flexBorder">
								<div class="header">
					<h3>Device Type</h3>
					<div class="information">
					{{{informationImg}}}
					<div class="informationContent">
					  {{{info_device}}}
					</div>
				  </div>
				</div>
				<div class="chartArea">
					{{{viz_device_type_year}}}
				</div>
			</div>
			<div class="flex1">
			  <div class="header">
					<h3>User Time on Page</h3>
					<div class="information">
					{{{informationImg}}}
					<div class="informationContent">
					  {{{info_ave_time}}}
					</div>
				  </div>
				</div>
				<div class="chartArea">
					{{{app_time}}}
				</div>	
			</div>
		</section>
		<section class="half flexContainer">
			<div class="flex1 flexBorder">
				<div class="header">
					<h3>New vs Returning Users</h3>
					<div class="information">
						{{{informationImg}}}
						<div class="informationContent">
						  {{{info_new_return}}}
						</div>
					</div>
				</div>
				<div class="chartArea">
					{{{new_vs_returning_year}}}
				</div>
			</div>
			<div class="flex1">
				<div class="header">
					<h3>Time-of-Day Activity</h3>
					<div class="information">
					{{{informationImg}}}
					<div class="informationContent">
					  {{{info_time_of_day}}}
					</div>
				  </div>
				</div>
				<div class="chartArea">
					{{{timeDayUse_app_year}}}
				</div>
			</div>
		</section>
		<section class="disclaimerGlossary">
		  <div class="header">
				<h3>Disclaimer/Glossary</h3>
			</div>
		  {{{disclaimerGlossaryText}}}
		</section>
	</div><!--content-->
</div>
  