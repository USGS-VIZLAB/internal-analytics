<div class="container flexContainer">
  {{{sideBar}}}
  <div class="content flex5">
    <section class="full">
      <div class="header">
				<h3>Sessions</h3>
				<div class="information">
				    {{{informationImg}}}
				    <div class="informationContent">
				      {{{info_sessions}}}
				    </div>
				  </div>
			</div>
			<div class="chartArea">
				{{{portfolio_sessions_all}}}
			</div>
    </section>
<section id="lineAssessments" class="full">
			<div class="header">
				<h3>Timeline Summaries</h3>
				<div class="information">
					{{{informationImg}}}
					<div class="informationContent">
						{{{info_session_time}}}
