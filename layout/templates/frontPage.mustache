<div class="container">
  <div id="sliders" class="flexContainer">
		<div id="projectListHolder" class="slider">
		  <div id="projectList">
  			<h4>
  			  Projects List 
  			  <div class="arrow">
  			    {{{arrow}}}
  			  </div>
  			</h4>
  		  <div id="projects">
    		  <p>Bio Data</p>
    		  <p>Bio Retrieval</p>
    		  <p>Coastal Chnage Hazards</p>
  		  </div>
		  </div>
		</div>
		<div class="disclaimerHolder slider flex1">
		  <div class="disclaimer">
  			<h4>
  			  About this report
  			  <div class="arrow">
  			    {{{arrow}}}
  			  </div>
  			</h4>
  			<div class="disclaimerText">
  			  <p>{{{aboutThisReport}}}</p>
  			  <h3>{{{disclaimerTitle}}}</h3>
  			  <p>{{{disclaimer1}}}</p>
  			  <p>{{{disclaimer2}}}</p>
  			</div>
			</div>
		</div>
	</div>
  <div class="topHalf">
    <div id="sessions" class="topicHeader flexContainer box">
      <h2>Sessions</h2>
      <div id="users" class="box flex1">
        new & returning users bar
      </div>
    </div>
    <div id="Timelines" class="flexContainer">
      <div id="year" class="sessionTimeline flex1 box">
        <h3>Yearly</h3>
        <p>{{year1}} - {{year2}}</p>
        <div class="timelinePlot">
        {{{portfolio_year_sessions}}}
        </div>
      </div>
      <div id="month" class="sessionTimeline flex1 box">
        <h3>Monthly</h3>
        <p>{{month1}} - {{month2}}</p>
        <div class="timelinePlot">
        {{{portfolio_month_sessions}}}
        </div>
      </div>
      <div id="week" class="sessionTimeline flex1 box">
        <h3>Weekly</h3>
        <p>{{week1}} - {{week2}}</p>
        <div class="timelinePlot">
        {{{portfolio_week_sessions}}}
        </div>
      </div>
      <div id="day" class="sessionTimeline flex1 box">
        <h3>Daily</h3>
        <p>-</p>
        <div class="timelinePlot">
        {{{portfolio_day_sessions}}}
        </div>
      </div>
    </div>
  </div>
  <div class="bottomHalf flexContainer">
    <div id="geo" class="box flex1">
      <div class="topicHeader">
        <h2>Geo</h2>
      </div>
      <div id="geoMap" class="box">
        map
      </div>
    </div>
    <div id="deviceTraffic" class="box flex1">
      <div class="topicHeader">
        <h2>Device Type</h2>
      </div>
      <div id="deviceType" class="box"></div>
      <div class="topicHeader">
        <h2>Traffic</h2>
      </div>
      <div id="traffic" class="box"></div>
    </div>
  </div>
  <div class="glossary">
	  <h3>{{{glossaryTitle}}}</h3>
	  <p>{{{session}}}</p>
	  <p>{{{visitors}}}</p>
	  <p>{{{geographical}}}</p>
	  <p>{{{average}}}</p>
	  <p>{{{traffic}}}</p>
	  <p>{{{device}}}</p>
	</div>
</div>