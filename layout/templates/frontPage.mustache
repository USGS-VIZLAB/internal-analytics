<div class="container">
  <div id="sliders" class="flexContainer">
		<div id="projectListHolder" class="slider">
		  <div id="projectList">
  			<h4>
  			  Web applications 
  			  <div class="arrow">
  			    {{{arrow}}}
  			  </div>
  			</h4>
        {{{projects}}}
		  </div>
		</div>
		<div class="disclaimerHolder slider flex1">
		  <div class="disclaimer">
  			<h4>
  			  About this report
  			  <div class="arrow">
  			    {{{arrow}}}
  			  </div>
  			</h4>
  			<div class="disclaimerText">
  			  <p>{{{aboutThisReport}}}</p>
  			  <h3>{{{disclaimerTitle}}}</h3>
  			  <p>{{{disclaimer1}}}</p>
  			  <p>{{{disclaimer2}}}</p>
  			</div>
			</div>
		</div>
	</div>
  <div class="topHalf">
    <div id="sessions" class="topicHeader flexContainer box">
      <h2>Sessions</h2>
      <div id="users" class="box flex1">
        new & returning users bar
      </div>
    </div>
    <div id="Timelines" class="flexContainer">
      <div id="year" class="sessionTimeline flex1 box">
        <div class="timelinePlot">
          {{{portfolio_sessions_all}}}
        </div>
      </div>
    </div>
  </div>
  <div class="bottomHalf flexContainer">
    <div id="geo" class="box flex1">
      <div class="topicHeader">
        <h2>Geographic Location</h2>
      </div>
      <div id="geoMap" class="box">
        {{{viz_geo_portfolio}}}
      </div>
    </div>
    <div id="deviceTraffic" class="box flex1">
      <div class="topicHeader">
        <h2>Device Type</h2>
      </div>
      <div id="deviceType" class="box">
      {{{portfolio_device_type}}}
      </div>
      <div class="topicHeader">
        <h2>Traffic Source</h2>
      </div>
      <div id="traffic" class="box">
      {{{portfolio_source}}}
      </div>
      <div id="timeDay" class="box">
      {{{timeDayUse_port}}}
      </div>
      <div id="AveTime" class="box">
      {{{portfolio_time}}}
      </div>
    </div>
  </div>
  <div class="glossary">
	  <h3>{{{glossaryTitle}}}</h3>
	  <p>{{{session}}}</p>
	  <p>{{{visitors}}}</p>
	  <p>{{{geographical}}}</p>
	  <p>{{{average}}}</p>
	  <p>{{{traffic}}}</p>
	  <p>{{{device}}}</p>
	</div>
</div>