			<div class="flex1 flexBorder">
			  <div class="header">
					<h3>New vs Returning Users</h3>
					<div class="information">
						{{{informationImg}}}
						<div class="informationContent">
						  {{{info_new_return}}}
						</div>
					</div>
				</div>
				<div class="chartArea">
					{{{portfolio_new_return_year}}}
				</div>
			</div>
			<div class="flex1">
				<div class="header">
					<h3>Time-of-Day Activity</h3>
					<div class="information">
					{{{informationImg}}}
					<div class="informationContent">
					  {{{info_time_of_day}}}
					</div>
				  </div>
				</div>
				<div class="chartArea">
					{{{portfolio_timeDayUse_year}}}
				</div>
			</div>
		</section>
		<section class="disclaimerGlossary">
		  <div class="header">
				<h3>Disclaimer/Glossary</h3>
			</div>
		  {{{disclaimerGlossaryText}}}
		</section>
  </div><!--content-->
