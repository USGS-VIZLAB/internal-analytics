<div class="container flexContainer">
  {{{sideBar}}}
  <div class="content">
    <div id="sessions" class="topicHeader">
      <h3>Sessions</h3>
    </div>
    <div id="timeline">
      <div class="timelinePlot">
        {{{portfolio_sessions_all}}}
      </div>
    </div>
    <div id="geo">
      <div class="topicHeader">
        <h3>Geographic Location</h3>
      </div>
      <div id="geoMap">
        {{{viz_geo_portfolio}}}
      </div>
    </div>
    <div id="deviceTraffic" class="flexContainer">
      <div id="deviceType" class="flex1">
        <div class="topicHeader">
          <h3>Device Type</h3>
        </div>
        <div id="deviceTypeChart">
          {{{portfolio_device_type}}}
        </div>
      </div>
      <div id="traffic" class="flex1">
        <div class="topicHeader">
          <h3>Traffic Source</h3>
        </div>
        <div id="trafficChart">
          {{{portfolio_source}}}
        </div>
      </div>
    </div><!--deviceTraffic-->
    <div id="time" class="flexContainer">
      <div id="timeDay" class="flex1">
        <div class="topicHeader">
          <h3>Time of Day</h3>
        </div>
        <div id="timeDayChart">
          {{{timeDayUse_port}}}
        </div>
      </div>
      <div id="aveTime" class="flex1">
        <div class="topicHeader">
          <h3>Average Time</h3>
        </div>
        <div id="AveTimeChart">
          {{{portfolio_time}}}
        </div>
      </div>
    </div>
    {{{disclaimerGlossaryText}}}
  </div><!--content-->
</div><!--container-->